    //play and pause video function
    function isInView(el) {
      var rect = el.getBoundingClientRect(); // absolute position of video element
      return !(rect.top > $(window).height() || rect.bottom < 0); // visible(True) or invisible(False)
    }
    $(document).on("scroll", function () {
      $("video").each(function () {
        if (isInView($(this)[0])) {
          // if visible (on screen)
          var figure = $(".vid-cont").hover(hoverVideo, hideVideo);
          var figure2 = $(".navbar").hover(hoverVideo, hideVideo);
          function hoverVideo(e) {
            $("video", this).get(0).play();
            $(".fader").fadeOut(500);
          }

          function hideVideo(e) {
            $("video", this).get(0).pause();
            $(".fader").fadeIn(500);
          }
        } else {
          // if invisible (out of the screen)
          if (!$(this)[0].paused) {
            $(this)[0].pause();
            $(".fader").fadeIn(500);
          }
        }
      });
    });

