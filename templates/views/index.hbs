  <div id="modal-wrapper" class="modal">
    <div class="login-wrap">
      <div class="login-html">
        <i class="fas fa-times fa-lg" style="
              float: right;
              margin-top: -70px;
              margin-right: -50px;
              color: white;
              cursor: pointer;
            " onclick="document.getElementById('modal-wrapper').style.display = 'none'"></i>
        <input id="tab-1" type="radio" name="tab" class="sign-in" checked /><label for="tab-1" class="tab">Sign
          In</label>
        <input id="tab-2" type="radio" name="tab" class="sign-up" /><label for="tab-2" class="tab">Sign Up</label>

        <div class="login-form">
          <!-- sign in -->
          <form class="needs-validation" novalidate name="signInForm" onsubmit="signInCheck()">
            <div class="sign-in-htm">
              <div class="group">
                <label for="validationCustom01" class="label">Username</label>
                <input name="id" type="text" class="form-control input" id="validationCustom01 user"
                  placeholder="Username" required />
                <div class="invalid-feedback">
                  Please provide Username
                </div>
                <!-- <label for="user" class="label">Username</label>
                            <input id="user" type="text" class="input"> -->
              </div>

              <div class="group">
                <label for="validationCustom02" class="label">Password</label>
                <input name="password" type="password" data-type="password" class="form-control input"
                  id="validationCustom02 pass" placeholder="Password" required />
                <div class="invalid-feedback">
                  Please provide Password
                </div>
              </div>

              <div class="group">
                <input id="check" type="checkbox" class="check" checked />
                <label for="check"><span class="icon"></span> Remember me</label>
              </div>

              <div class="group">
                <button class="button" type="submit" style="width: 100%;">
                  Sign In
                </button>
              </div>
              <div class="hr"></div>
            </div>
          </form>

          <!-- sign up -->
          <form class="needs-validation" novalidate name="signUpForm">
            <div class="sign-up-htm">
              <div class="group">
                <label for="validationCustom01 user" class="label">Username</label>
                <input type="text" class="form-control input" id="validationCustom01 user" placeholder="Username"
                  required />
                <div class="invalid-feedback">
                  Please provide Username
                </div>
              </div>

              <div class="group">
                <label for="validationCustom02" class="label">Password</label>
                <input type="password" data-type="password" class="form-control input" id="validationCustom02 pass"
                  placeholder="Password" required />
                <div class="invalid-feedback">
                  Please provide Password
                </div>
              </div>

              <div class="group">
                <label for="validationCustom03" class="label">Confirm Password</label>
                <input type="password" data-type="password" class="form-control input" id="validationCustom03 pass"
                  placeholder="Confirm Password" required />
                <div class="invalid-feedback">
                  Incorrect Password
                </div>
