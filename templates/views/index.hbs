      var TextInsideLi = ctrl.getElementsByTagName("p")[0].innerHTML;
      if (TextInsideLi.length > 11) {
        TextInsideLi = TextInsideLi.slice(0, 10) + "...";
      }
      if (document.getElementById("theatername").innerHTML === TextInsideLi) {
        document.getElementById("theatername").innerHTML = "Select Theater";
      } else {
        document.getElementById("theatername").innerHTML = TextInsideLi;
      }
    }
    function setMovie(ctrl) {
      var TextInsideLi = ctrl.getElementsByTagName("p")[0].innerHTML.trim();
      if (TextInsideLi.length > 11) {
        TextInsideLi = TextInsideLi.slice(0, 10) + "...";
      }
      if (document.getElementById("moviename").innerHTML === TextInsideLi) {
        document.getElementById("moviename").innerHTML = "Select Movie";
      } else {
        document.getElementById("moviename").innerHTML = TextInsideLi;
      }
    }

    function bookingNav() {
      if (
        document.getElementById("theatername").innerHTML ===
        "Select Theater" ||
        document.getElementById("moviename").innerHTML === "Select Movie"
      ) {
        alert("Please Select Theater and Movie");
      } else window.location = "process_1";
    }
    function bookingPop() {
      window.location = "process_1";
    }
    function signInCheck() {
      var id = document.forms["signInForm"]["id"].value;
      var pas = document.forms["signInForm"]["password"].value;
      if (id != "admin" || pas != "admin") {
        alert("Invalid ID or password. Please try again.");
      }
    }
  </script>
